<table class="table">
  <thead>
    <tr>
      <th>Venue</th>
      <th>Five Pound Notes</th>
      <th>One Pound Coins</th>
      <th>Fifty Pence Coins</th>
      <th>Twenty Pence Coins</th>
      <th>Ten Pence Coins</th>
      <th>Five Pence Coins</th>
      <th></th>
    <tr>
  </thead>
  <tbody>
    <% change_orders.each do |change_order| %>
      <tr>
        <td><%= change_order.venue.name %></td>
        <td><%= change_order.five_pound_notes %></td>
        <td><%= change_order.one_pound_coins %></td>
        <td><%= change_order.fifty_pence_coins %></td>
        <td><%= change_order.twenty_pence_coins %></td>
        <td><%= change_order.ten_pence_coins %></td>
        <td><%= change_order.five_pence_coins %></td>
        <td>
          <%= render partial: action_partial, locals: { change_order: change_order } %>
        </td>
      </tr>
      <tr>
        <td></td>
        <td><%= number_to_currency(change_order.five_pound_total, unit: '£', precision: 2) %></td>
        <td><%= number_to_currency(change_order.one_pound_total, unit: '£', precision: 2) %></td>
        <td><%= number_to_currency(change_order.fifty_pence_total, unit: '£', precision: 2) %></td>
        <td><%= number_to_currency(change_order.twenty_pence_total, unit: '£', precision: 2) %></td>
        <td><%= number_to_currency(change_order.ten_pence_total, unit: '£', precision: 2) %></td>
        <td><%= number_to_currency(change_order.five_pence_total, unit: '£', precision: 2) %></td>
        <td></td>
      </tr>
      <tr>
        <td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td>
      </tr>
    <% end %>
  </tbody>
</table>
