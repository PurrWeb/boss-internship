<%= form.text_field(
    :week_start_date,
    value: form.object.week_start_date && UIRotaDate.format(form.object.week_start_date),
    label: 'Week',
    class: 'date-picker'
  ) %>
<%= form.number_field(:hours, min: 0, max: 119) %>
<%= form.number_field(:minutes, min: 0, max: 59) %>
<%= form.text_area :note %>
