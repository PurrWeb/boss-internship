<% if !user.security_manager? %>
  <%= form.select(
        :venues,
        options_for_select(
          Venue.all.map { |venue| [venue.name.titleize, venue.id] },
          form.object.venues.map{ |venue| venue.id }
        ),
        {
          label: 'Venues'
        },
        multiple: true,
        id: 'staff-member-venues-select',
        class: 'chosen-select'
      ) %>
<% end %>
