<% if current_user.present? %>
  <%= react_component(
    'Header',
    {
      quickMenu: header_data[:quick_menu],
      venues: header_data[:venues],
      user: {
        id: logged_in_user_info_json[:id],
        name: logged_in_user_info_json[:name]
      }
    }
  )%>
<% else %>
  <%= react_component(
    'EmptyHeader'
  )%>
<% end %>
