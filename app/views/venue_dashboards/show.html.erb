<%= react_component('VenueDashboardApp', {
  accessToken: access_token.token,
  currentVenue: Api::V1::VenueDashboard::VenueSerializer.new(current_venue),
  renderWeatherWidget: render_weather_widget,
  venues: ActiveModel::Serializer::CollectionSerializer.new(
    venues,
    serializer: Api::V1::VenueDashboard::VenueSerializer,
  ),
  messages: ActiveModel::Serializer::CollectionSerializer.new(
    messages,
    serializer: Api::V1::VenueDashboard::MessageSerializer,
  ),
}) %>
