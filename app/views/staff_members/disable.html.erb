<h1>Disable Staff Member</h1>

<% if staff_member.user.present? %>
  <div class="user-disable-warning">
    <p class="text-warning">
      Warning! This staff member has an associated user. Disabling will also disable the user.
    </p>
  </div>
<% end %>

<%= render partial: 'shared/disable_staff_member_form', locals: { url: staff_member_path(staff_member), form: form } %>
