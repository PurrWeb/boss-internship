<%= javascript_tag do %>
    window.boss = window.boss || {};
    window.boss.store = window.boss.store || {};
    window.boss.store.accessToken = "<%= access_token.token %>";
    window.boss.store.venueValues = <%= venues.map{ |v| { name: v.name, id: v.id } }.to_json.html_safe %>;
    window.boss.store.payrateValues = <%= pay_rates.map { |pr| { name: pr.name, id: pr.id } }.to_json.html_safe %>;
    window.boss.store.staffTypeIds = <%= staff_types.map{ |st| { name: st.name, id: st.id } }.to_json.html_safe %>;
    window.boss.store.genderValues = <%= gender_values.to_json.html_safe %>;
<% end %>

<%= react_component(
        "AddStaffMemberPageComponent"
    ) %>
